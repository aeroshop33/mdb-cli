<!-- Modal Banners -->       
<div id="modal_ayuda_div"></div>

<script>
    $(function () {
        $("#password_image_btn").on('mouseup touchend mouseout', function () {
            var button_image = $(this).children('i');
            button_image.removeClass().addClass('fa fa-eye');
            $("#password_input").attr("type", "password");

        });
        $("#password_image_btn").on('mousedown touchstart', function () {
            var button_image = $(this).children('i');
            button_image.removeClass().addClass('fa fa-eye-slash');
            $("#password_input").attr("type", "text");
        });
    });

    function modal_ayuda(id) {
        event.preventDefault();
        $.ajax({
            type: "POST",
            datatype: 'text',
            cache: false,
            headers: {
                'X-CSRF-Token': csrfToken
            },
            beforeSend: function () {
                $('#modal_ayuda_div').html('');
            },
            data: {
                id: id
            },
            url: "/cuentas/ayuda-modal",
            success: function (data) {
                $('#modal_ayuda_div').html(data);
                $('#modal_ayuda').modal("show");
            },
            complete: function () {

            }
        });
    }

    function modal_ayuda_dismiss() {
        $('#modal_ayuda').modal("hide");
        $('#modal_ayuda').html('');
    }
</script>
