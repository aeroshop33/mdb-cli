    $(function () {

        // Evita la doble sumbiteada de un formulario al hacer dos veces click
        var isSubmit = false;
        if ($("form[data-toggle='validator']").length !== 0) {
            $("form[data-toggle='validator']").validator().on('submit', function (e) {
                if (!e.isDefaultPrevented()) {
                    if (isSubmit)
                        return false;
                    isSubmit = true;
                }
            });
        } else if ($("form").length !== 0)
        {
            $('form').submit(function () {
                if (isSubmit)
                    return false;
                isSubmit = true;
            });
        }


        // deshabilita la rueda del mouse sobre los campos number
        $(':input[type=number]').on('mousewheel', function (e) {
            $(this).blur();
        });

    });
